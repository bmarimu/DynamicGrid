{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "./src/",
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "module": "amd",
    "typeRoots": [ "node_modules/@types" ]
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "./Source/Typings/globals/*",
    "obj",
    "bin"
  ]
}